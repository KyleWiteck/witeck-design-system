{
  "name": "@KyleWiteck/witeck-design",
  "version": "0.2.4",
  "license": "UNLICENSED",
  "author": "Magnit - Kyle Witeck & Francisco Veracoechea",
  "description": "The Magnit React Design Library node module",
  "type": "module",
  "node": ">=18",
  "sideEffects": [
    "**/*.css",
    "**/*.woff",
    "**/*.woff2",
    "**/*.png"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": "https://github.com/peopleticker/magnit-design",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*/index.d.ts",
        "./dist/index.d.ts"
      ]
    }
  },
  "engines": {
    "yarn": ">=3.5.0",
    "npm": ">=9.6.7",
    "node": ">=18.17.0"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsc && vite build --watch",
    "generate-icons": "npx tsx generate-icons.ts",
    "optimize-icons": "npx tsx optimize-icons.ts",
    "build": "npm run generate-icons && tsc && vite build"
  },
  "keywords": [
    "design",
    "library",
    "system"
  ],
  "peerDependencies": {
    "@types/react": "*",
    "@types/react-dom": "*",
    "react": "^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.5",
    "@radix-ui/react-popover": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.1.7",
    "@radix-ui/react-visually-hidden": "^1.1.1",
    "@vanilla-extract/css": "^1.16.1",
    "@vanilla-extract/recipes": "^0.5.5",
    "@vanilla-extract/sprinkles": "^1.6.3",
    "react": "^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-popper": "^2.3.0"
  },
  "devDependencies": {
    "@laynezh/vite-plugin-lib-assets": "^0.5.26",
    "@rollup/plugin-typescript": "^11.1.6",
    "@vanilla-extract/vite-plugin": "^4.0.18",
    "@vitejs/plugin-react": "^4.3.4",
    "esbuild": "^0.14.43",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "svgo": "^3.3.2",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vite-plugin-dts": "^4.3.0",
    "vite-plugin-static-copy": "^2.2.0",
    "vite-plugin-svgr": "^4.3.0"
  },
  "exports": {
    "./global.css": "./dist/style.css",
    "./components": {
      "types": "./dist/components.d.ts",
      "default": "./dist/components.js"
    },
    "./hooks": {
      "types": "./dist/hooks.d.ts",
      "default": "./dist/hooks.js"
    },
    "./theme": {
      "types": "./dist/theme.d.ts",
      "default": "./dist/theme.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "default": "./dist/types.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "default": "./dist/utils.js"
    },
    "./icons": {
      "types": "./dist/icons.d.ts",
      "default": "./dist/icons.js"
    }
  }
}
